FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0.993 0.993 0.000);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform
        #include        "include/inletU"
        ;
    }
    
    outlet
    {
        type            zeroGradient;
    }
    
    ground
    {
        type             noSlip;
    }
    
    sky
    {
        type             slip;
    }
    
    sides
    {
        type             slip;
    }
    
    "proc.*"
    {
        type            processor;
    }
}

// ************************************************************************* //
